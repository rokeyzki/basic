{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "env": {
        "browser": true,
        "node": true,
        "jquery": true
    },
    "rules": {
        // `"off"` 或 `0` - 关闭规则
        // `"warn"` 或 `1` - 开启规则，使用警告级别的错误：warn (不会导致程序退出)
        // `"error"` 或 `2` - 开启规则，使用错误级别的错误：error (当被触发的时候，程序会退出)
        "linebreak-style": [0, "unix"],
        "no-console": [0],
        "jsx-a11y/no-static-element-interactions": [0],
        "import/no-extraneous-dependencies": [0]
    }
}
